<h1>Add a new trip</h1>

<form class="new" action="/trips" method="post">
  <div>
    <label for="name">Name</label>
    <input type="text" name="name">
  </div>
  <div>
    <label for="startDate">Start date</label>
    <input type="text" name="startDate">
  </div>
  <button>Submit</button>
</form>

<a href="/trips">Back</a>
